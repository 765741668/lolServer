<binding xmlns:ns1="http://www.yzh.hkjc.hk/Schema/Test/Jibx/ShipOrder_V1" xmlns:ns3="http://www.yzh.hkjc.hk/Schema/Test/Jibx/Common" trim-whitespace="true">
  <namespace uri="http://www.yzh.hkjc.hk/Schema/Test/Jibx/ShipOrder_V1" default="elements"/>
  <mapping abstract="true" type-name="ns1:shiptotype" class="com.lol.jibx.shiporderv1.Shiptotype">
    <value style="element" name="name" get-method="getName" set-method="setName"/>
    <value style="element" name="address" get-method="getAddress" set-method="setAddress"/>
    <value style="element" name="city" get-method="getCity" set-method="setCity"/>
    <value style="element" name="country" get-method="getCountry" set-method="setCountry" format="ns1:countryType"/>
  </mapping>
  <mapping abstract="true" type-name="ns1:itemtype" class="com.lol.jibx.shiporderv1.Itemtype">
    <value style="element" name="title" get-method="getTitle" set-method="setTitle"/>
    <value style="element" name="note" get-method="getNote" set-method="setNote" usage="optional"/>
    <value style="element" name="quantity" get-method="getQuantity" set-method="setQuantity"/>
    <value style="element" name="price" get-method="getPrice" set-method="setPrice"/>
  </mapping>
  <mapping abstract="true" type-name="ns1:CTshiporder" class="com.lol.jibx.shiporderv1.CTshiporder">
    <structure map-as="ns3:absOrder"/>
    <value style="element" name="orderperson" get-method="getOrderperson" set-method="setOrderperson"/>
    <structure map-as="ns1:shiptotype" get-method="getShipto" set-method="setShipto" name="shipto"/>
    <collection get-method="getItemList" set-method="setItemList" create-type="java.util.ArrayList">
      <structure map-as="ns1:itemtype" name="item"/>
    </collection>
    <value style="attribute" name="orderid" get-method="getOrderid" set-method="setOrderid"/>
  </mapping>
  <mapping class="com.lol.jibx.shiporderv1.CTshiporder" name="shiporder">
    <structure map-as="ns1:CTshiporder"/>
  </mapping>
</binding>