<binding xmlns:ns2="http://www.yzh.hkjc.hk/Schema/Test/Jibx/ChuanOrder_v1" xmlns:ns3="http://www.yzh.hkjc.hk/Schema/Test/Jibx/Common" trim-whitespace="true">
  <namespace uri="http://www.yzh.hkjc.hk/Schema/Test/Jibx/ChuanOrder_v1" default="elements"/>
  <mapping abstract="true" type-name="ns2:postaddr" class="com.lol.jibx.chuanorderv1.Postaddr">
    <value style="element" name="name" get-method="getName" set-method="setName"/>
    <value style="element" name="address" get-method="getAddress" set-method="setAddress"/>
    <value style="element" name="city" get-method="getCity" set-method="setCity"/>
    <value style="element" name="country" get-method="getCountry" set-method="setCountry" format="ns2:country"/>
  </mapping>
  <mapping abstract="true" type-name="ns2:chuanItemType" class="com.lol.jibx.chuanorderv1.ChuanItemType">
    <value style="element" name="title" get-method="getTitle" set-method="setTitle"/>
    <value style="element" name="note" get-method="getNote" set-method="setNote" usage="optional"/>
    <value style="element" name="quantity" get-method="getQuantity" set-method="setQuantity"/>
    <value style="element" name="price" get-method="getPrice" set-method="setPrice"/>
  </mapping>
  <mapping abstract="true" type-name="ns2:chuanOrder" class="com.lol.jibx.chuanorderv1.ChuanOrder">
    <structure map-as="ns3:absOrder"/>
    <value style="element" name="orderperson" get-method="getOrderperson" set-method="setOrderperson"/>
    <structure map-as="ns2:postaddr" get-method="getChuanto" set-method="setChuanto" name="chuanto"/>
    <collection get-method="getItemList" set-method="setItemList" create-type="java.util.ArrayList">
      <structure map-as="ns2:chuanItemType" name="item"/>
    </collection>
    <value style="attribute" name="orderid" get-method="getOrderid" set-method="setOrderid"/>
  </mapping>
  <mapping class="com.lol.jibx.chuanorderv1.ChuanOrder" name="chuanorder">
    <structure map-as="ns2:chuanOrder"/>
  </mapping>
</binding>