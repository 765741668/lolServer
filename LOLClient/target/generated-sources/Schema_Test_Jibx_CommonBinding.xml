<binding xmlns:ns3="http://www.yzh.hkjc.hk/Schema/Test/Jibx/Common" trim-whitespace="true">
  <namespace uri="http://www.yzh.hkjc.hk/Schema/Test/Jibx/Common" default="elements"/>
  <mapping abstract="true" type-name="ns3:absOrder" class="com.lol.jibx.common.AbsOrder">
    <value style="element" name="updateTime" get-method="getUpdateTime" set-method="setUpdateTime"/>
    <value style="element" name="deleted" get-method="isDeleted" set-method="setDeleted"/>
    <value style="element" name="id" get-method="getId" set-method="setId"/>
    <collection get-method="getMapList" set-method="setMapList" usage="optional" create-type="java.util.ArrayList">
      <structure map-as="ns3:maptype" name="map"/>
    </collection>
  </mapping>
  <mapping abstract="true" type-name="ns3:maptype" class="com.lol.jibx.common.Maptype">
    <value style="element" name="key" get-method="getKey" set-method="setKey"/>
    <value style="element" name="value" get-method="isValue" set-method="setValue"/>
  </mapping>
</binding>